<launch>
  <!-- Launch RQT Multi-plot -->
  <node name="mpc_observation"
        pkg="rqt_multiplot"
        type="rqt_multiplot"
        args="--multiplot-run-all --multiplot-config $(find ocs2_cartpole)/config/multiplot/mpc_observation.xml"
        output="screen"/>

  <!-- Launch Performance Indices Multi-plot -->
  <include file="$(find ocs2_ros_interfaces)/launch/performance_indices.launch">
    <arg name="mpc_policy_topic_name"      value="cartpole_mpc_policy"/>
  </include>
</launch>
